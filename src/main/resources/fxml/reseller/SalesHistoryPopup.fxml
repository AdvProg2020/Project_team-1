<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane minHeight="400.0" minWidth="400.0" styleClass="popup-pane" stylesheets="@../Common.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.graphical.Reseller">
   <bottom>
      <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#onCloseClick" styleClass="close-button" text="Close" BorderPane.alignment="CENTER" />
   </bottom>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <top>
      <Label styleClass="scene-title" text="Title" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <ScrollPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <TableView prefHeight="276.0" prefWidth="776.0">
              <columns>
                <TableColumn prefWidth="116.0" text="Received money" />
                <TableColumn prefWidth="168.0" text="Deducted money on sale" />
                  <TableColumn prefWidth="61.0" text="Buyer" />
                  <TableColumn prefWidth="75.0" text="Delivered" />
                  <TableColumn prefWidth="75.0" text="ID" />
                  <TableColumn prefWidth="50.0" text="Date" />
                  <TableColumn prefWidth="100.0" text="Commodities" />
              </columns>
            </TableView>
         </content>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </ScrollPane>
   </center>
</BorderPane>
