<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<ScrollPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal"
            fx:controller="client.view.graphical.commodity.Compare">
    <AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="popup-pane">
        <children>
            <GridPane fx:id="compareGridPane" layoutX="328.0" layoutY="219.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <columnConstraints>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="0.0" prefWidth="200.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="500.0"/>
                    <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="500.0"/>
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints minHeight="10.0" prefHeight="200.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                    <RowConstraints minHeight="10.0" prefHeight="30.0" valignment="CENTER" vgrow="SOMETIMES"/>
                </rowConstraints>
                <children>
                    <Label styleClass="hint-label" text="Name" GridPane.rowIndex="1"/>
                    <Label styleClass="hint-label" text="Brand" GridPane.rowIndex="2"/>
                    <Label styleClass="hint-label" text="Price" GridPane.rowIndex="3"/>
                    <Label styleClass="hint-label" text="Rating" GridPane.rowIndex="4"/>
                    <Label styleClass="hint-label" text="Description" GridPane.rowIndex="5"/>
                    <Label fx:id="commodityName" styleClass="hint-label" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
                    <Label fx:id="secondCommodityName" styleClass="hint-label" GridPane.columnIndex="2"
                           GridPane.rowIndex="1"/>
                    <Label fx:id="commodityBrand" styleClass="hint-label" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
                    <Label fx:id="secondCommodityBrand" styleClass="hint-label" GridPane.columnIndex="2"
                           GridPane.rowIndex="2"/>
                    <Label fx:id="commodityPrice" styleClass="hint-label" GridPane.columnIndex="1"
                           GridPane.rowIndex="3"/>
                    <Label fx:id="secondCommodityPrice" styleClass="hint-label" GridPane.columnIndex="2"
                           GridPane.rowIndex="3"/>
                    <Label fx:id="commodityRating" styleClass="hint-label" GridPane.columnIndex="1"
                           GridPane.rowIndex="4"/>
                    <Label fx:id="secondCommodityRating" styleClass="hint-label" GridPane.columnIndex="2"
                           GridPane.rowIndex="4"/>
                    <ImageView fx:id="commodityImage" fitHeight="200.0" fitWidth="150.0" pickOnBounds="true"
                               preserveRatio="true" GridPane.columnIndex="1"/>
                    <ImageView fx:id="secondCommodityImage" fitHeight="200.0" fitWidth="150.0" pickOnBounds="true"
                               preserveRatio="true" GridPane.columnIndex="2"/>
                </children>
            </GridPane>
            <Button layoutX="387.0" layoutY="780.0" mnemonicParsing="false" onAction="#back" styleClass="close-button"
                    text="Close" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="495.0"
                    AnchorPane.rightAnchor="495.0"/>
        </children>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <stylesheets>
            <URL value="@Common.css"/>
        </stylesheets>
    </AnchorPane>
</ScrollPane>
