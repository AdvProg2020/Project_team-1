<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane fitToWidth="true" minHeight="600.0" minViewportHeight="600.0" minViewportWidth="900.0" minWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.graphical.Reseller">
   <content>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="background" stylesheets="@Common.css">
         <children>
            <BorderPane prefHeight="800.0" prefWidth="1200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <top>
                  <Label fx:id="usernameLabel" styleClass="scene-title" text="Username" BorderPane.alignment="CENTER" />
               </top>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <center>
                  <VBox BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                     <children>
                        <HBox alignment="CENTER" prefHeight="206.0" prefWidth="696.0" spacing="10.0">
                           <children>
                              <VBox alignment="CENTER" spacing="10.0">
                                 <children>
                                    <ImageView fx:id="userPhotoImageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                                       <image>
                                          <Image url="@../default-user-image.png" />
                                       </image>
                                    </ImageView>
                                    <Button mnemonicParsing="false" onMouseClicked="#onChangeUserPhotoClick" styleClass="normal-button" text="Change user photo" />
                                 </children>
                                 <HBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </HBox.margin>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </VBox.margin>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                           <children>
                              <VBox alignment="CENTER" prefHeight="312.0" prefWidth="140.0" spacing="10.0">
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                                 <children>
                                    <Label styleClass="hint-label" text="First name :" />
                                    <Label layoutX="22.0" layoutY="337.0" styleClass="hint-label" text="Last name :" />
                                    <Label layoutX="22.0" layoutY="317.0" styleClass="hint-label" text="Email :" />
                                    <Label layoutX="22.0" layoutY="356.0" styleClass="hint-label" text="Phone number :" />
                                 </children>
                              </VBox>
                              <VBox alignment="CENTER" prefHeight="682.0" prefWidth="341.0" spacing="10.0">
                                 <children>
                                    <Label fx:id="firstNameLabel" onMouseClicked="#onFirstNameLabelClick" styleClass="hint-label" text="First name" />
                                    <Label fx:id="lastNameLabel" onMouseClicked="#onLastNameClick" styleClass="hint-label" text="Last name" />
                                    <Label fx:id="emailLabel" onMouseClicked="#onEmailClick" styleClass="hint-label" text="Email" />
                                    <Label fx:id="phoneNumberLabel" onMouseClicked="#onPhoneNumberClick" styleClass="hint-label" text="Phone number" />
                                 </children>
                                 <HBox.margin>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </HBox.margin>
                                 <padding>
                                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                 </padding>
                              </VBox>
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER" layoutX="20.0" layoutY="609.0" prefHeight="206.0" prefWidth="696.0" spacing="10.0">
                           <children>
                              <Button mnemonicParsing="false" onMouseClicked="#onChangePasswordClick" styleClass="normal-button" text="Change password" />
                           </children>
                           <padding>
                              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                           </padding>
                        </HBox>
                     </children>
                  </VBox>
               </center>
               <left>
                  <VBox alignment="BOTTOM_CENTER" spacing="10.0" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <Button mnemonicParsing="false" styleClass="logout-button" text="Logout" />
                        <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" styleClass="back-button" text="Back" textAlignment="CENTER" />
                     </children>
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </left>
               <right>
                  <VBox alignment="BOTTOM_CENTER" spacing="10.0" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <Button mnemonicParsing="false" styleClass="forward-button" text="Products" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                  </VBox>
               </right>
            </BorderPane>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
