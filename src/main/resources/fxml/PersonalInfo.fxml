<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane minHeight="400.0" minWidth="400.0" styleClass="popup-pane" stylesheets="@Common.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.graphical.Reseller">
   <bottom>
      <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#onCloseClick" styleClass="close-button" text="Close" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <VBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <Label styleClass="scene-title" text="Personal info" />
            <Label fx:id="usernameLabel" styleClass="hint-label" text="Username">
               <VBox.margin>
                  <Insets top="50.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="firstNameLabel" layoutX="150.0" layoutY="125.0" onMouseClicked="#onFirstNameClick" styleClass="hint-label" text="First name" />
            <Label fx:id="lastNameLabel" layoutX="150.0" layoutY="157.0" styleClass="hint-label" text="Last name" />
            <Label fx:id="emailLabel" layoutX="150.0" layoutY="189.0" styleClass="hint-label" text="Email" />
            <Label fx:id="phoneNumberLabel" layoutX="150.0" layoutY="221.0" styleClass="hint-label" text="Phone number" />
            <Label fx:id="phoneNumberLabel1" layoutX="133.0" layoutY="253.0" styleClass="hint-label" text="********" />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
