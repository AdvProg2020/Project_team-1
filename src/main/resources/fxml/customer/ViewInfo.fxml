        <Button mnemonicParsing="false" onAction="#editFirstName" prefHeight="48.0" prefWidth="250.0"
                styleClass="normal-button" text="Edit first name" AnchorPane.rightAnchor="10.0"
                AnchorPane.topAnchor="10.0"/>
        <Button mnemonicParsing="false" onAction="#editLastName" prefHeight="48.0" prefWidth="250.0"
