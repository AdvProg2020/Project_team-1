<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fitToHeight="true" fitToWidth="true" minHeight="600.0" minViewportHeight="600.0" minViewportWidth="900.0" minWidth="900.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.graphical.Reseller">
   <content>
      <AnchorPane prefHeight="800.0" prefWidth="1200.0" styleClass="background" stylesheets="@Common.css">
         <children>
            <BorderPane prefHeight="800.0" prefWidth="1200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <top>
                  <Label styleClass="scene-title" text="Reseller panel" BorderPane.alignment="CENTER" />
               </top>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <center>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <children>
                        <Label fx:id="resellerBalanceLabel" styleClass="hint-label" text="Your balance" />
                        <Button mnemonicParsing="false" onMouseClicked="#onPersonalInfoClick" styleClass="normal-button" text="Personal info" />
                        <Button mnemonicParsing="false" onMouseClicked="#onCompanyInfoClick" styleClass="normal-button" text="Company info" />
                        <Button mnemonicParsing="false" onMouseClicked="#onSalesHistoryClick" styleClass="normal-button" text="Sales history" />
                        <Button mnemonicParsing="false" onMouseClicked="#onManageProductsClick" styleClass="normal-button" text="Manage products" />
                        <Button mnemonicParsing="false" onMouseClicked="#onCategoriesClick" styleClass="normal-button" text="Categories" />
                        <Button mnemonicParsing="false" styleClass="normal-button" text="Manage offs" />
                     </children>
                  </VBox>
               </center>
               <left>
                  <VBox alignment="BOTTOM_CENTER" spacing="10.0" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <Button mnemonicParsing="false" onMouseClicked="#onLogoutClick" styleClass="logout-button" text="Logout" />
                        <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" styleClass="back-button" text="Back" textAlignment="CENTER" />
                     </children>
                     <opaqueInsets>
                        <Insets top="10.0" />
                     </opaqueInsets>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </left>
               <right>
                  <VBox alignment="BOTTOM_CENTER" spacing="10.0" BorderPane.alignment="BOTTOM_CENTER">
                     <children>
                        <Button mnemonicParsing="false" styleClass="forward-button" text="Products" />
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <BorderPane.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </BorderPane.margin>
                  </VBox>
               </right>
            </BorderPane>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
