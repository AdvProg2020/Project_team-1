                  <Button mnemonicParsing="false" styleClass="normal-button" text="Categories" />
                  <Button mnemonicParsing="false" styleClass="normal-button" text="Balance" />
                  <Button mnemonicParsing="false" styleClass="normal-button" text="Manage offs" />
               </children>
            </VBox>
         </center>
         <left>
            <Button alignment="BOTTOM_CENTER" mnemonicParsing="false" styleClass="back-button" text="Back" textAlignment="CENTER" BorderPane.alignment="BOTTOM_CENTER" />
         </left>
         <right>
            <Button mnemonicParsing="false" styleClass="forward-button" text="Products" BorderPane.alignment="BOTTOM_CENTER" />
         </right>
      </BorderPane>
   </children>
</AnchorPane>
